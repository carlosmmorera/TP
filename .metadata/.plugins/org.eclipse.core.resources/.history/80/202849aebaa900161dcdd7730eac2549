package TP.PR1.MV;

public class ByteCodeParser {
	public static ByteCode parse(String s){
		String[] subcadenas=s.split(" ");
		ByteCode bc;
		ENUM_BYTECODE ebc;
		
		switch (subcadenas[0]){
		case "push": ebc=ENUM_BYTECODE.PUSH; break;
		case "load": ebc=ENUM_BYTECODE.LOAD; break;
		case "store": ebc=ENUM_BYTECODE.STORE; break;
		case "add": ebc=ENUM_BYTECODE.ADD; break;
		case "sub": ebc=ENUM_BYTECODE.SUB; break;
		case "mul": ebc=ENUM_BYTECODE.MUL; break;
		case "div": ebc=ENUM_BYTECODE.DIV; break;
		case "out": ebc=ENUM_BYTECODE.OUT; break;
		case "halt": ebc=ENUM_BYTECODE.HALT; break;
		default: return null;
		}
//		if (subcadenas[0].equals("add")) ebc=ENUM_BYTECODE.ADD;
//		else if (subcadenas[0].equals("push")) ebc=ENUM_BYTECODE.PUSH;
//		else if (subcadenas[0].equals("load")) ebc=ENUM_BYTECODE.LOAD;
//		else if (subcadenas[0].equals("store")) ebc=ENUM_BYTECODE.STORE;
//		else if (subcadenas[0].equals("sub")) ebc=ENUM_BYTECODE.SUB;
//		else if (subcadenas[0].equals("mul")) ebc=ENUM_BYTECODE.MUL;
//		else if (subcadenas[0].equals("div")) ebc=ENUM_BYTECODE.DIV;
//		else if (subcadenas[0].equals("out")) ebc=ENUM_BYTECODE.OUT;
//		else if (subcadenas[0].equals("halt")) ebc=ENUM_BYTECODE.HALT;
//		else return null;

		if (subcadenas.length == 1) bc=new ByteCode(ebc);
		else if (subcadenas.length == 2){
			int par= Integer.parseInt(subcadenas[2]);
			bc=new ByteCode(ebc, par);
		}
		else return null;
		
		return bc;
	}
}
