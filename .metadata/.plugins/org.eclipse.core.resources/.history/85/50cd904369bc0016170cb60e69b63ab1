package tp.pr2.Arithmetics;

import tp.pr2.ByteCode.ByteCode;
import tp.pr2.CPU.CPU;

public class Div extends Arithmetics{
	public boolean execute(CPU cpu){
		int op2 = cpu.pilapop();
		boolean correctexe = false;
		
		if (op2 != 0 ) correctexe = cpu.push(cpu.pilapop() / op2);
		if (correctexe) cpu.aumentarContProgram();
		
		return correctexe;
	}
	public ByteCode parse(String[] s){
		if (s[0].equalsIgnoreCase("DIV")) return new Div();
		else return null;
	}
}
