package TP.PR1.MV;

public class OperandStack {
	private int[] stack;
	private int numoperand;
	
	public OperandStack(){
		this.stack=new int[100];
		this.numoperand=0;
	}
	public void pushop(int op){
		this.stack[this.numoperand]= op;
		++this.numoperand;
	}
	public boolean pop(int valor){
		if (numoperand > 0){
			valor=this.stack[numoperand - 1];
			--numoperand;
			return true;
		}
		else return false;
	}
}
