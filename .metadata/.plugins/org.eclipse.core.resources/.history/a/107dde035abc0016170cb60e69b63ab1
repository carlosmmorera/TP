package tp.pr2.ConditionalJumps;

import tp.pr2.ByteCode.ByteCode;

public class IfEq extends ConditionalJumps{
	private int pos;
	public IfEq(int n){
		this.pos = n;
	}
	public IfEq(){
		this.pos = 0;
	}
	public ByteCode parse(String[] s){
		if (s[0].equalsIgnoreCase("IFEQ")) return new IfEq(Integer.parseInt(s[1]));
		else return null;
	}
}
